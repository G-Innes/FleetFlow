{
  "version": 2,
  "builds": [
    { "src": "public/index.php", "use": "vercel-php@0.7.4" },
    { "src": "public/build/**", "use": "@vercel/static" },
    { "src": "public/favicon.ico", "use": "@vercel/static" },
    { "src": "public/favicon.svg", "use": "@vercel/static" },
    { "src": "public/logo.svg", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/favicon.svg", "dest": "/public/favicon.svg" },
    { "src": "/logo.svg", "dest": "/public/logo.svg" },
    { "src": "/(.*)", "dest": "public/index.php" }
  ],
  "installCommand": "npm ci --legacy-peer-deps",
  "buildCommand": "composer install --no-dev --optimize-autoloader && npm run build"
  ,
  "env": {
    "APP_NAME": "Fleet Fox Task Management",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://fleet-flow-gilt.vercel.app",
    "APP_KEY": "base64:2MKeILbEfhqxkcRL7y+MENBjL0j9tAG6hv8wWxZfPvc=",
    "DB_CONNECTION": "pgsql",
    "DB_HOST": "ep-lively-tree-agqntlqb-pooler.c-2.eu-central-1.aws.neon.tech",
    "DB_PORT": "5432",
    "DB_DATABASE": "neondb",
    "DB_USERNAME": "neondb_owner",
    "DB_PASSWORD": "npg_dt8EQqvW9ONo",
    "DB_SSLMODE": "require",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "LOG_CHANNEL": "stderr",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "VIEW_COMPILED_PATH": "/tmp"
  }
}
