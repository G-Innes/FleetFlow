{
  "version": 2,
  "builds": [
    { "src": "public/index.php", "use": "vercel-php@0.7.4" },
    { "src": "public/build/**", "use": "@vercel/static" },
    { "src": "public/favicon.ico", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/(.*)", "dest": "public/index.php" }
  ],
  "installCommand": "npm ci --legacy-peer-deps",
  "buildCommand": "composer install --no-dev --optimize-autoloader && npm run build"
  ,
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "VIEW_COMPILED_PATH": "/tmp"
  }
}
