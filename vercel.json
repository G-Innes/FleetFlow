{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.7.4"
    }
  ],
  "buildCommand": "composer install --no-dev --optimize-autoloader && npm install && npm run build",
  "installCommand": "composer install --no-dev --optimize-autoloader",
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "public/build/$1"
    },
    {
      "src": "/favicon\\.ico",
      "dest": "public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_NAME": "Fleet Fox Task Management",
    "APP_URL": "https://fleet-flow-ohr9w0keu-g-innes-projects.vercel.app",
    "APP_KEY": "base64:2MKeILbEfhqxkcRL7y+MENBjL0j9tAG6hv8wWxZfPvc=",
    "DB_CONNECTION": "pgsql",
    "DB_HOST": "ep-lively-tree-agqntlqb-pooler.c-2.eu-central-1.aws.neon.tech",
    "DB_PORT": "5432",
    "DB_DATABASE": "neondb",
    "DB_USERNAME": "neondb_owner",
    "DB_PASSWORD": "npg_dt8EQqvW9ONo",
    "DB_SSLMODE": "require",
    "SESSION_DRIVER": "database",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "LOG_CHANNEL": "stderr",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "VERCEL_FORCE_NO_BUILD_CACHE": "1"
  }
}

